{
  "name": "dominion",
  "version": "1.0.0",
  "description": "A customizable DNS server written in node.js",
  "main": "dist/index.js",
  "repository": "https://github.com/theprometeus/dominion",
  "author": "Matheus Giovani",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "start": "node --expose-gc dist/index.js",
    "watch": "ts-node-dev --watch --expose-gc -r tsconfig-paths/register src/index.ts",
    "watch:debug": "ts-node-dev --watch --expose-gc --debug --inspect -r tsconfig-paths/register src/index.ts"
  },
  "dependencies": {
    "express": "^4.18.1",
    "nocodb": "^0.92.3",
    "sequelize": "^6.21.2",
    "sequelize-typescript": "^2.1.3",
    "sqlite3": "^5.0.8",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@types/sequelize": "^4.28.13",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
